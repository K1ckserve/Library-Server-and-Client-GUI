<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.main.GUI.Controller">
   <children>
      <Button fx:id="Login" layoutX="230.0" layoutY="285.0" mnemonicParsing="false" onAction="#loginButtonAction" text="Login" />
      <TextField fx:id="username" layoutX="226.0" layoutY="143.0" onAction="#loginButtonAction" prefHeight="12.0" prefWidth="149.0" />
      <TextField fx:id="password" layoutX="226.0" layoutY="222.0" onAction="#loginButtonAction" />
      <Text layoutX="226.0" layoutY="119.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" />
      <Text layoutX="228.0" layoutY="196.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" />
      <Button fx:id="ForgotPassword" layoutX="452.0" layoutY="349.0" mnemonicParsing="false" onAction="#forgotPassword" text="Forgot Password" />
      <Button fx:id="New" layoutX="317.0" layoutY="285.0" mnemonicParsing="false" onAction="#newUser" text="Sign Up" />
   </children>
</AnchorPane>
